<Attrs>
    <Attr name="language">
        <Scopes>
            <Function/>
        </Scopes>
        <Type>
            <Enumeration defaultValue="SDL">
                <Entry value="C++"/>
                <Entry value="Ada"/>
                <Entry value="SDL"/>
            </Enumeration>
        </Type>
    </Attr>
    <Attr name="is_type">
        <Type>
            <Enumeration defaultValue="NO">
                <Entry value="YES"/>
                <Entry value="NO"/>
            </Enumeration>
        </Type>
        <Scopes>
            <Function/>
        </Scopes>
    </Attr>
    <Attr name="instance_of">
        <Type>
            <String validator="[a-zA-Z0-9_]+"/>
        </Type>
        <Scopes>
            <Function>
                <AttrValidator name="is_type" value="False"/>
            </Function>
        </Scopes>
    </Attr>
    <Attr name="period">
        <Type>
            <String validator="\d+"/>
        </Type>
        <Scopes>
            <Required_Interface>
                <AttrValidator name="kind" value="Cyclic"/>
            </Required_Interface>
            <Provided_Interface>
                <AttrValidator name="kind" value="Cyclic"/>
            </Provided_Interface>
        </Scopes>
    </Attr>
    <Attr name="wcet">
        <Type>
            <String validator="\d+"/>
        </Type>
        <Scopes>
            <Required_Interface>
                <AttrValidator name="kind" value="Cyclic"/>
            </Required_Interface>
            <Provided_Interface>
                <AttrValidator name="kind" value="Cyclic"/>
            </Provided_Interface>
        </Scopes>
    </Attr>
    <Attr name="queue_size">
        <Type>
            <String validator="\d+"/>
        </Type>
        <Scopes>
            <Required_Interface>
                <AttrValidator name="kind" value="Cyclic"/>
            </Required_Interface>
            <Provided_Interface>
                <AttrValidator name="kind" value="Cyclic"/>
            </Provided_Interface>
        </Scopes>
    </Attr>
    <Attr name="Taste::coordinates" type="property" visible="false">
        <Type>
            <String validator="[0-9 ]+"/>
        </Type>
        <Scopes/>
    </Attr>
    <Attr name="Taste::InnerCoordinates" type="property" visible="false">
        <Type>
            <String validator="[0-9 ]+"/>
        </Type>
        <Scopes/>
    </Attr>
    <Attr name="Taste::RootCoordinates" type="property" visible="false">
        <Type>
            <String validator="[0-9 ]+"/>
        </Type>
        <Scopes>
            <Function/>
        </Scopes>
    </Attr>
    <Attr name="Taste::InheritPI" type="property">
        <Type>
            <Boolean/>
        </Type>
        <Scopes>
            <Required_Interface/>
        </Scopes>
    </Attr>
    <Attr name="Taste::Autonamed" type="property">
        <Type>
            <Boolean defaultValue="True"/>
        </Type>
        <Scopes>
            <Required_Interface/>
        </Scopes>
    </Attr>
    <Attr label="UIText" name="LabelTest" type="property">
        <Type>
            <Boolean/>
        </Type>
        <Scopes>
            <Function/>
        </Scopes>
    </Attr>
</Attrs>
