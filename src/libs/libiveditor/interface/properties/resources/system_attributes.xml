<Attrs>
    <Attr name="name">
        <Type>
            <String defaultValue="AAAA" validator="[a-zA-Z0-9_]+"/>
        </Type>
        <Scopes/>
    </Attr>
    <Attr name="value">
        <Type>
            <String validator="[a-zA-Z0-9_]+"/>
        </Type>
        <Scopes/>
    </Attr>
    <Attr name="kind">
        <Scopes>
            <Required_Interface/>
            <Provided_Interface/>
        </Scopes>
        <Type>
            <Enumeration>
                <Entry value="Cyclic"/>
                <Entry value="Sporadic"/>
                <Entry value="Protected"/>
                <Entry value="Unprotected"/>
            </Enumeration>
        </Type>
    </Attr>
</Attrs>
